.node
  box-sizing border-box
  position absolute
  overflow hidden
  border 1px solid grey

.node-label
  padding 4px
  line-height 1em
  white-space pre

.node-value
  color rgba(0,0,0,0.8)
  font-size 9px
  margin-top 1px
 
 .entities-container 
 	position relative
 
.ent-container
	position absolute
	width 100%	
	display flex
	justify-content space-between
	align-items baseline
	top	110px

.ent-container h4
	color #127B5F
	text-align center

.ent-container a:nth-child(3)
	filter grayscale(100%)
 
