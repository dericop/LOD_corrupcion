	font-size 17px
	width 215px

.depto-info
	position absolute
	top 20px
	height 200px
	width 100%
	text-align center
	
.depto-label
	font-weight 500

.depto-name
	font-weight 300

#map
	position absolute
	margin auto
	left 0
	right 0
	
.prot-note	
	position absolute
	top 80%
	font-weight 300



