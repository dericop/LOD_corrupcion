/* Estilos Department */
.background
	fill transparent
	
rect.background
	stroke-width 0;

.depto-container .breadcrump
	background url("../../assets/images/resources.png") -330px 1666px
	width 260px
	height 40px

a
	text-decoration none

a:hover
	text-decoration none

.breadcrump
	position absolute
	width 100%
	height 40px
	top 1px
	left -5px
	z-index 10
	
.breadcrump-decorator
	position absolute
	background url("../../assets/images/resources.png") -231px -626px
	width 69px
	height 40px

.breadcrump-right-decorator
	position	 absolute
	background url("../../assets/images/resources.png") -343px -630px
	left 39px
	height 33px
	width 216px
	top 4px
	

.breadcrump-data
	position relative
	width 100%
	height 100%

.breadcrump-data h4
	color #fff
	padding 0
	margin 0

.breadcrump-data a:first-child
	position absolute
	top 15px
	left 10px

.breadcrump-data a:first-child > h4
	font-size 15px
	font-weight 300

.breadcrump-data a:nth-child(2)
	position absolute
	top 11px
	left 82px

.breadcrumo-container
	position relative

.breadcrump-data a:nth-child(2) > h4
	font-size 18px
	font-weight 400

.depto-gen
	min-height 600px 

.dept-view 
	position relative
	width 100%
	height 100%
	min-height 600px


.top-info
	position absolute
	top 40px
	left 23px

.top-info p
